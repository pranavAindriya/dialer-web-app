{
  "version": 2,
  "builds": [
    {
      "src": "api/ssr.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/ssr.js"
    }
  ],
  "outputDirectory": "dist/client"
}
